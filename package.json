{
  "name": "simple-peer-wrapper",
  "version": "1.0.0",
  "description": "A wrapper for simple-peer that includes a socket.io signaling server client. To be used in the browser with simple-peer-server on your server.",
  "repository": "https://github.com/lisajamhoury/simple-peer-wrapper",
  "main": "dist/simple-peer-wrapper.js",
  "scripts": {
    "build": "browserify src/index.js --standalone SimplePeerWrapper > dist/simple-peer-wrapper.js",
    "watch": "watchify src/index.js --standalone SimplePeerWrapper -o dist/simple-peer-wrapper.js"
  },
  "keywords": [],
  "author": "lisajamhoury",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^16.5.0",
    "watchify": "^3.11.1"
  },
  "dependencies": {
    "simple-peer": "^9.6.0",
    "socket.io-client": "^2.3.0"
  }
}
